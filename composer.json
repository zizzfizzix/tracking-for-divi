{
  "name": "digitallstudio/divi-form-tracking",
  "description": "Track only successful Divi contact form submits through the dataLayer.",
  "type": "wordpress-plugin",
  "require": {
    "kucrut/vite-for-wp": "^0.5.2"
  },
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "DigitallStudio\\DiviFormTracking\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Kuba Serafinowski",
      "email": "kuba.serafinowski@digitall.studio"
    }
  ],
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "phpcompatibility/phpcompatibility-wp": "*",
    "wp-coding-standards/wpcs": "^2.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
